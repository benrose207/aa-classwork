<h1>All cats</h1>
<% @cats.each do |cat| %>
    <ul>
        <li><%= cat.name %></li>
        <li><%= cat.color%></li>
        <li><%= cat.birth_date%></li>
        <li><%= cat.sex%></li>
        <li><%= cat.description%></li>

        <li><a href="<%= cat_url(cat)%>">View Cat</a></li>
    </ul>
<% end %>


